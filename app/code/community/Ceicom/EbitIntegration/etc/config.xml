<config>
	<modules>
		<Ceicom_EbitIntegration>
			<version>0.0.1</version>
		</Ceicom_EbitIntegration>
	</modules>
	<global>
		<blocks>
	     <ceicom_ebitintegration>
	         <class>Ceicom_EbitIntegration_Block</class>
	     </ceicom_ebitintegration>
	  </blocks>
		<helpers>
			<EbitIntegration>
				<class>Ceicom_EbitIntegration_Helper</class>
			</EbitIntegration>
		</helpers>
	</global>
	<frontend>
		<layout>
			<updates>
				<ceicom_ebitintegration>
					<file>ceicom/ebitintegration.xml</file>
				</ceicom_ebitintegration>
			</updates>
		</layout>
	</frontend>
	<default>
		<EbitIntegration>
			<config>
				<habilitar>0</habilitar>
				<EbitIntegration_storeid></EbitIntegration_storeid>
				<EbitIntegration_buscapeid></EbitIntegration_buscapeid>
				<EbitIntegration_popup>1</EbitIntegration_popup>
			</config>
		</EbitIntegration>
	</default>
</config>
